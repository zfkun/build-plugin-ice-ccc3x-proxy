    "@ice/app": "^3.0.0",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zfkun/build-plugin-ice-ccc3x-proxy.git"
  },
  "publicConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
